"#view_subnotes":{
	layout: "composite",
}

"#viewsub":{
	layout: "vertical",
	width: '90%',
	height: Ti.UI.SIZE,
	borderRadius: 10,
	elevation: 10,
}

"#name":{
	color: "black",
	font: {
		fontWeight: "semibold",
		fontSize: 30,
		fontFamily: 'Helvetica Neue'
	},
}

"#viewtext":{
	borderRadius: 10,
	top:"1%",
	width: '96%',
	height: '70%',
	backgroundColor: "#e6e6e6",
	borderColor: "black",
	layout: "vertical"
}


"#textArea":{
	width: '96%',
	height: '100%',
	color:"black",
	font: {
		fontWeight: "normal",
		fontSize: 26,
		fontFamily: 'Helvetica Neue'
	},
	verticalAlign: Ti.UI.TEXT_VERTICAL_ALIGNMENT_TOP,
	includeFontPadding : true
}

"#erase":{
	top:"2%",
	width: "90%",
}


